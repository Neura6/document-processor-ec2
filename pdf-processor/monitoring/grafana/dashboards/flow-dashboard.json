{
  "dashboard": {
    "id": null,
    "title": "PDF Processing Flow",
    "tags": ["pdf", "flow"],
    "timezone": "browser",
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Files in SQS Queue",
        "type": "stat",
        "targets": [
          {
            "expr": "sqs_files_queued",
            "legendFormat": "Queued"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Files Processing (EC2)",
        "type": "stat",
        "targets": [
          {
            "expr": "ec2_files_processing",
            "legendFormat": "Processing"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Files Completed",
        "type": "stat",
        "targets": [
          {
            "expr": "ec2_files_completed_total",
            "legendFormat": "Completed"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Files Synced to KB",
        "type": "stat",
        "targets": [
          {
            "expr": "kb_files_synced_total",
            "legendFormat": "Synced"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 0
        }
      }
    ],
    "schemaVersion": 27,
    "version": 1
  }
}
