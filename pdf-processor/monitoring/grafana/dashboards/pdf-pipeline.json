{
  "dashboard": {
    "id": null,
    "title": "PDF Processing Pipeline",
    "tags": ["pdf", "processing", "pipeline"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Files Processed Today",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(pdf_files_processed_total[24h])",
            "legendFormat": "Files Processed"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(pdf_files_processed_total{status=\"success\"}[1h]) / rate(pdf_files_processed_total[1h]) * 100",
            "legendFormat": "Success Rate %"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Average Processing Time",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.5, rate(pdf_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "Avg Time"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Queue Depth",
        "type": "stat",
        "targets": [
          {
            "expr": "sqs_messages_in_queue",
            "legendFormat": "Messages in Queue"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Processing Volume Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(pdf_files_processed_total[5m])",
            "legendFormat": "Files per Minute"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4}
      },
      {
        "id": 6,
        "title": "Success Rate Trend",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(pdf_files_processed_total{status=\"success\"}[5m]) / rate(pdf_files_processed_total[5m]) * 100",
            "legendFormat": "Success Rate %"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4}
      },
      {
        "id": 7,
        "title": "Processing Steps Performance",
        "type": "table",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(pdf_processing_duration_seconds_bucket[5m])) by (step)",
            "legendFormat": "95th percentile {{step}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12}
      },
      {
        "id": 8,
        "title": "Error Analysis",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum(rate(pdf_processing_errors_total[1h])) by (error_type)",
            "legendFormat": "{{error_type}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12}
      },
      {
        "id": 9,
        "title": "S3 Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(s3_operations_total[5m])",
            "legendFormat": "{{operation}} {{status}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20}
      },
      {
        "id": 10,
        "title": "KB Sync Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(kb_sync_total[5m])",
            "legendFormat": "{{status}} {{folder}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}
